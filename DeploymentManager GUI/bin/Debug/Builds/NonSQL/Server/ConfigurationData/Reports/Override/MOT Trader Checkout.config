<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>MOT Trader Checkout</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchMTVDealPricesPhysical, Motiva.RightAngle.Server.Trading.Reports</Class>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVDealPricesPhysicalReportEditManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>#MTVPhysicalDealArchiveEntries</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <HideReportInReportDesktop>false</HideReportInReportDesktop>
    <IsReferenceDataReport>false</IsReferenceDataReport>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Physical Trading</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      <!--Show prices where [b]Interface Date[/b] is between [0] and [1] and [b]Load Status[/b] is [2]-->
      Show deals with a [b]Status[/b] of [0] by [b]Trader[/b]  [1] [br]
      and the [b]Deal Number[/b] contains [2] [br] and [b]Strategy[/b]  [4] [br]
      and [b]Revision Date[/b] of [3]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="DealStatusComboBox" ID="0" ParameterName="@Status" Width="75" Filter="Id = 'A' OR Id = 'C'" />
  <Control DisplayStyle="InternalContactComboBox" ID="1" ParameterName="@DlDtlIntrnlUserID" />
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@ContractNumber" Width="100"/>
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@DealRevisionDate" />
  <Control DisplayStyle="StrategyComboBox" ID="4" ParameterName="@StrtgyID" />
</ReportConfiguration>
