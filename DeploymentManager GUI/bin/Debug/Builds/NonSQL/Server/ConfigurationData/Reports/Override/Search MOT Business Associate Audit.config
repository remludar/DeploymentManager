<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Business Associate Audit</ReportName>
    <Class>Motiva.RightAngle.Server.General.Reports.BusinessAssociateAuditReport, Motiva.RightAngle.Server.General.Reports</Class>
    <BusinessDomain>System</BusinessDomain>
    <ReportEntity></ReportEntity>
    <ReportCriteriaManagerClass></ReportCriteriaManagerClass>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>false</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>   
    <IncludeInToolsList>Toolbox</IncludeInToolsList>
    <ReportsToExcludeFromNavigation></ReportsToExcludeFromNavigation>
  </ReportDefinition>
	<CriteriaObject>
		<CriteriaName>ReportCriteria</CriteriaName>
		<CriteriaUIDefinition>
      Show Business Associate Audit details for BAs [br]
      with [b]Modifications / Changes[/b] made between [0] and [1] by [b]User[/b] [2] [br]
      with an [b]Business Associate Name[/b] [3], [b]Created[/b] between [4] and [5] [br]
    </CriteriaUIDefinition>
		<CriteriaUIClass></CriteriaUIClass>
	</CriteriaObject>
  <Control DisplayStyle="DateTimeShowTime" ID="0" ParameterName="@ModificationFromDate" />
  <Control DisplayStyle="DateTimeShowTime" ID="1" ParameterName="@ModificationToDate" />
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@ModificationUserName" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@BusinessAssociateName" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@CreatedFromDate" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@CreatedToDate" />
</ReportConfiguration>


