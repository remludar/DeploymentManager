<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>MOT OAS Truck Ticket Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.MTVOASTruckTicketsMaintenance, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVOASTruckTicketStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Scheduling.EditableReportManagers.MTVOASTruckTicketStagingReportEditManager, Motiva.RightAngle.Client.Scheduling.EditableReportManagers</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Scheduling</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Truck Tickets with [b] Movement Product [/b] [1] [br]
      and [b] Movement Date [/b] [2] between [3] and [br]
      [b] Status [/b] [4]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="ProductComboBox" ID="1" ParameterName="@ProductName" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@MovementDateStart" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@MovementDateEnd" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="4" ParameterName="@TicketStatus" Width="100"/>
</ReportConfiguration>