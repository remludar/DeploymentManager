<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT TMS BOL Staging</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchMTVTMSBol, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Interface</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <ReportEntity>MTVTMSMovementStaging</ReportEntity>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Reporting.Criteria.DateReportCriteriaManager, SolArc.RightAngle.Client</ReportCriteriaManagerClass>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show BOLs with a [b]Ticket Status [/b] [0] and a [b]Load Date [/b] [1] between [2] [br]
      and [b]BOL Number[/b] [3] for [b] Product Code [/b] [4] [br]
      [b] Interfaced [/b] [5] between [6]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="0" ParameterName="@TicketStatus" Width="100"/>
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@LoadStart" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@LoadEnd" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@BOLNumber" Width="200"/>
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@ProductCode" Width="150"/>
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@InterfaceEnd" />
</ReportConfiguration>
