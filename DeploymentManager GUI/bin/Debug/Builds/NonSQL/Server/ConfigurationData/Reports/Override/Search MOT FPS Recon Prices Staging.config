<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT FPS Recon Prices Staging</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchFPSReconPriceStagingReport, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <ReportEntity>MTVTransferPriceStaging</ReportEntity>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Reporting.Criteria.DateReportCriteriaManager, SolArc.RightAngle.Client</ReportCriteriaManagerClass>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Pricing &amp; Valuation</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Reconciled Prices for [b] Curve Type [/b] [0] where [b]Interface Date [/b] [1] between [2] [br]
      [b] Quote From Date [/b] [3] between [4] and [b] Quote To Date [/b] [5] between [6] [br]
      and [b] Plant [/b] Equals [7] and [b] SAP Material Code [/b] Equals [8] [br]
      with [b] Interface Status [/b] Equals [9]
      
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVFPSReconPriceComboBox" ID="0" ParameterName="@CurveType"/>
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@InterfaceEnd" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@QuoteFromStart" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@QuoteFromEnd" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@QuoteToStart" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@QuoteToEnd" />
  <Control DisplayStyle="MTVSAPPlantCodeComboBox" ID="7" ParameterName="@PlantCode" />
  <Control DisplayStyle="MTVSAPMaterialCodeComboBox" ID="8" ParameterName="@SAPMaterialCode" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="9" ParameterName="@InterfaceStatus" Width="100" />
 </ReportConfiguration>
