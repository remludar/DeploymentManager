<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
	<ReportDefinition>
		<ReportName>Search MOT Manual Movement Load</ReportName>
		<Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchMTVManualMovementLoad, Motiva.RightAngle.Server.Scheduling.Reports</Class>
		<BusinessDomain>Scheduling</BusinessDomain>
		<ReportEntity>RawPriceDetail</ReportEntity>
		<DataBaseConnectionName></DataBaseConnectionName>
		<ExternalCriteriaName></ExternalCriteriaName>
		<AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<ShowReportInNavigation>true</ShowReportInNavigation>
		<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
		<IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
	</ReportDefinition>
	<CriteriaObject>
		<CriteriaName>ReportCriteria</CriteriaName>
		<CriteriaUIDefinition>
			Show movements where [b]Interface Date[/b] is between [0] and [1] and [b]Load Status[/b] is [2]
		</CriteriaUIDefinition>
		<CriteriaUIClass />
	</CriteriaObject>
	<Control DisplayStyle="DateTime" ID="0" ParameterName="@InterfaceFromDate" />
	<Control DisplayStyle="DateTime" ID="1" ParameterName="@InterfaceToDate" />
	<Control DisplayStyle="MTVInterfaceStatusComboBox" ID="2" ParameterName="@LoadStatus" />
</ReportConfiguration>