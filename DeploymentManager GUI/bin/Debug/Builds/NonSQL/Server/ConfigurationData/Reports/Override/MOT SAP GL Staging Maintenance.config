<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<ReportDefinition>
		<ReportName>MOT SAP GL Staging Maintenance</ReportName>
		<Class>Motiva.RightAngle.Server.Accounting.Reports.SearchSAPGLStaging, Motiva.RightAngle.Server.Accounting.Reports</Class>
		<BusinessDomain>Accounting</BusinessDomain>
    <ReportEntity>MTVSAPGLStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Accounting.EditableReports.MTVSAPGLStagingReportEditManager, Motiva.RightAngle.Client.Accounting.EditableReports</ReportEditManagerClass>
    <ExternalCriteriaName></ExternalCriteriaName>
		<AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<ShowReportInNavigation>true</ShowReportInNavigation>
		<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Payable Invoices for [b]External BA[/b] [0] [br]
      with [b]posting date[/b] between [1] and [2] [br]
      and a [b]RightAngle interface status[/b] of [3] [br]
      and a [b]Middleware interface status[/b] of [4] [br]
      and an [b]SAP interface status[/b] of [5] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateExternalAllDistinctComboBox" ID="0" ParameterName="@ExternalBAID" />
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@PostingFromDate" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@PostingToDate" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="3" ParameterName="@RAInterfaceStatus" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="4" ParameterName="@MWInterfaceStatus" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="5" ParameterName="@SAPInterfaceStatus" />
</ReportConfiguration>