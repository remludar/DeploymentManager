<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT TMS to SAP Product Mapping</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchTMSProductToSAPMaterialCode, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Reference Data</BusinessDomain>
    <ReportEntity>MTVTMSSAPMaterialCodeXRef</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show TMS 6 digit Product Code to SAP 9 digit Material Codes for
      [b]Terminal [/b] [1] and [b]Material Code [/b] [2] [br]
      and [b]TMS Product Code [/b] [3] and [b]Sequence [/b] [4] [br]
      with [b]Status [/b] [5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVSAPPlantCodeDescriptionComboBox" ID="1" ParameterName="@RALocaleID" />
  <Control DisplayStyle="MTVSAPMaterialCodeDescriptionComboBox" ID="2" ParameterName="@RAProductID" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@TMSCode" />
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@Sequence" />
  <Control DisplayStyle="MTVActiveInactiveComboBox" ID="5" ParameterName="@Status" />
</ReportConfiguration>