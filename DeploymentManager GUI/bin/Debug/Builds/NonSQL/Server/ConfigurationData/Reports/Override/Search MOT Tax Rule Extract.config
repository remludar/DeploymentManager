<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Tax Rule Extract</ReportName>
    <Class>Motiva.RightAngle.Server.Accounting.Reports.MTVTaxRuleExtractReport, Motiva.RightAngle.Server.Accounting.Reports</Class>
    <BusinessDomain>Tax</BusinessDomain>
    <ReportEntity>TaxRuleSet</ReportEntity>
    <ReportCriteriaManagerClass>Motiva.RightAngle.Client.General.CriteriaManagers.TaxRuleExtractReportCriteriaManager, Motiva.RightAngle.Client.General.CriteriaManagers</ReportCriteriaManagerClass>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>false</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Accounting, Tax</IncludeInToolsList>
    <ReportsToExcludeFromNavigation></ReportsToExcludeFromNavigation>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>

      Show [b] Tax Rule Sets [/b] located under folder  [0]  AND [br]

      [b] Tax Rule Sets [/b] that are grouped under [1]  AND  [br]

      [b] Tax Rule Sets [/b] with a description of [2] [br]

    </CriteriaUIDefinition>
    <CriteriaUIClass></CriteriaUIClass>
  </CriteriaObject>
  <Control DisplayStyle="TaxFolderDescriptionComboBox" ID="0" ParameterName="@vc_TaxFolder" Width="150"/>
  <Control DisplayStyle="MTVTaxGroupingsComboBox" ID="1" ParameterName="@vc_TaxGrouping"  Width="200"/>
  <Control DisplayStyle="MTVTaxAuditRuleSetDescriptionComboBox" ID="2" ParameterName="@i_TaxRuleSet" Width ="300"/>
</ReportConfiguration>