<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT FSM Transfer Price Comparison</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchFsmTransferPriceComparison, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing</BusinessDomain>
    <ReportEntity>#MTVSTLFSMTransferPrice</ReportEntity>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Reporting.Criteria.DateReportCriteriaManager, SolArc.RightAngle.Client</ReportCriteriaManagerClass>
    <IncludeInToolsList>>Pricing &amp; Valuation</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Compare Transfer Prices To Actual for [b]Cost Category: YT01 [/b] at [b]Locations: [/b][0] for [b]Products: [/b][1] as of [b]Date [/b][2] [br]
      [b]Show Only Matched Prices[/b] [3]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="LocationComboBox" ID="0" ParameterName="@vc_LocaleIds" Width="200"/>
  <Control DisplayStyle="ProductComboBox" ID="1" ParameterName="@vc_ProductIds" Width="200" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@sdt_DateTime" Width="200" />
  <Control DisplayStyle="YesNoComboBox"  ID="3" ParameterName="@ShowMatched" Width="150"/>
</ReportConfiguration>