<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT TMS to SAP Class Of Trade Mapping</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchTMSToSAPClassOfTradeMappings, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Reference Data</BusinessDomain>
    <ReportEntity>MTVTMSClassOfTradeXRef</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show TMS Host Code to SAP Distribution Channel Mappings for
      [b]Distribution Channel [/b] [1] and [b]Class of Trade [/b] [2] [br]
      and [b]TMS Host Code [/b] [3] and [b]Commercial Fuel Indicator [/b] [4] [br]
      and [b]Brand Type [/b] [5] [br]
      [b]Effective[/b] between [6] and [7]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVDistributionChannelComboBox" ID="1" ParameterName="@DistributionChannel" />
  <Control DisplayStyle="MTVSAPBASoldToClassOfTradeComboBox" ID="2" ParameterName="@ClassOfTrade" />
  <Control DisplayStyle="MTVTMSHostCodeComboBox" ID="3" ParameterName="@TMSHostCode" />
  <Control DisplayStyle="YesNoComboBox" ID="4" ParameterName="@CommercialFuelIndicator" />
  <Control DisplayStyle="MTVTMSBrandTypeComboBox" ID="5" ParameterName="@BrandType" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@EffectiveFrom" />
  <Control DisplayStyle="DateTime" ID="7" ParameterName="@EffectiveTo" />
</ReportConfiguration>