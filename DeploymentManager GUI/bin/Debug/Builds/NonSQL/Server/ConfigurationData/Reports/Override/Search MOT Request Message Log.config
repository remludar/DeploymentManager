<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Request Message Log</ReportName>
    <Class>Motiva.RightAngle.Server.General.Reports.SearchMOTRequestMessageLog, Motiva.RightAngle.Server.General.Reports</Class>
    <BusinessDomain>System</BusinessDomain>
    <ReportEntity>#RequestMessageLogResults</ReportEntity>
    <IsReferenceDataReport>false</IsReferenceDataReport>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Toolbox</IncludeInToolsList>
    <ReportsToExcludeFromNavigation />
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Request Messages that were [b]Sent[/b] between [0] and [1][br]
      with the [b]Token[/b] [2] and are of [b]Type[/b] [3] [br]
      and are a [b]Scheduled Task[/b] [4] and an [b]Event Type [/b] of [5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="DateTimeShowTime" ID="0" ParameterName="@dt_FromDate" />
  <Control DisplayStyle="DateTimeShowTime" ID="1" ParameterName="@dt_ToDate" />
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@vc_Token" Width="300"/>
  <Control DisplayStyle="MessageTypeComboBox" ID="3" ParameterName="@i_MessageType" Width="300"/>
  <Control DisplayStyle="ToolboxScheduledTaskComboBox" ID="4" ParameterName="@i_ScheduledTaskId" Width="300"/>
  <Control DisplayStyle="ToolboxEventTypeComboBox" ID="5" ParameterName="@vc_EventType" Width="300"/>
  <TableReference TableName="#RequestMessageLogResults" AliasName="{Request Message Log}"/>
</ReportConfiguration>