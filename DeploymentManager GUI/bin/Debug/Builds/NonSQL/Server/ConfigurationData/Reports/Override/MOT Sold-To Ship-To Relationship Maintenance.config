<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT Sold-To Ship-To Relationship Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchSoldToShipTo, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>MTVSAPSoldToShipTo</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVSAPSoldToShipToReportEditManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>false</ShowReportInNavigation>
    <!--    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity> -->
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>T</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show SAP SoldTo - ShipTo for [b]Business Associate[/b] [1] [br]
      and [b]Sold-To Number[/b] [2] and [b]Ship-To Number[/b] [3] [br]
      and [b]Care of[/b] [4] and [b]Class Of Trade[/b] [11] [br]
      at [b]Ship-To Location[/b] [5] [br]
      between [b]Sold-To Ship-To Dates[/b] [6] and [7] [br]
      with [b]Sold-To Ship-To Status[/b] [8] and [b]BA Sold-To Status[/b] [9] [br]
      which is [b]Currently[/b] [10]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="1" ParameterName="@BAID" />
  <Control DisplayStyle="MTVSAPBASoldToVendorNumberNoBARelationStaticDescriptionComboBox" ID="2" ParameterName="@SoldTo" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@ShipTo" />
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@CareOf" />
  <Control DisplayStyle="LocationComboBox" ID="5" ParameterName="@RALocaleID" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@SoldToShipToFromDate" />
  <Control DisplayStyle="DateTime" ID="7" ParameterName="@SoldToShipToToDate" />
  <Control DisplayStyle="ActiveInactiveComboBox" ID="8" ParameterName="@Status" />
  <Control DisplayStyle="ActiveInactiveComboBox" ID="9" ParameterName="@BASoldToStatus" />
  <Control DisplayStyle="MTVUsedUnusedOrAllComboBox" ID="10" ParameterName="@UsedUnused" />
  <Control DisplayStyle="MTVSAPBASoldToClassOfTradeComboBox" ID="11" ParameterName="@ClassOfTrade" />
</ReportConfiguration>