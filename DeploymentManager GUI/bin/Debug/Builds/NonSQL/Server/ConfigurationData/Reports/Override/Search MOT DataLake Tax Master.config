<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT DataLake Tax Master</ReportName>
    <Class>Motiva.RightAngle.Server.Tax.Reports.SearchDataLakeMasterTax, Motiva.RightAngle.Server.Tax.Reports</Class>
    <BusinessDomain>Tax</BusinessDomain>
    <ReportEntity>MTVDataLakeMasterTaxStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Data Lake Tax Master with [b]Processed Status [/b] [0]
      and [b]Business Associate[/b] [1] and [b]LicenseName [/b] [2]  [br]
      and [b]Exemption[/b] [3] and [b]DeferBilling [/b] [4]  [br]
      and [b] CreatedDate [/b] [5] between [6]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="0" ParameterName="@ProcessedStatus" Width="100"/>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="1" ParameterName="@BAID" Width="200"/>
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@LicenseName" Width="150"/>
  <Control DisplayStyle="YesNoComboBox" ID="3" ParameterName="@Exemption" Width="150"/>
  <Control DisplayStyle="YesNoComboBox" ID="4" ParameterName="@DeferBilling" Width="150"/>
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@CreatedDateStart" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@CreatedDateEnd" />
</ReportConfiguration>