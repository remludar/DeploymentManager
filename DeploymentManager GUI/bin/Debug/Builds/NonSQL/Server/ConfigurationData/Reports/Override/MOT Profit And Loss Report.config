<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT Profit And Loss Report</ReportName>
    <Class>Motiva.RightAngle.Server.Risk.Reports.MTVProfitAndLossReport, Motiva.RightAngle.Server.Risk.Reports</Class>
    <BusinessDomain>Risk</BusinessDomain>
    <ReportCriteriaManagerClass>Motiva.RightAngle.Client.General.CriteriaManagers.MTVPnLReportCriteriaManager, Motiva.RightAngle.Client.General.CriteriaManagers</ReportCriteriaManagerClass>
		<ReportEntity>#MTVProfitAndLoss</ReportEntity>
    <HideReportInReportDesktop>false</HideReportInReportDesktop>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <IsReferenceDataReport>false</IsReferenceDataReport>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <!--<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Pricing &amp; Risk</IncludeInToolsList>-->
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show records for [b]Snapshot[/b] [1] [b]Portfolio[/b] [2] [b]Internal BA[/b] [3][br]
      from [b]Accounting Period[/b] [4] to [5] and [b]Delivery Period[/b] [6] to [7] and [8] [b]Reverse/Rebooks[/b][br]
      Show only [b]Transaction Group/Type[/b] [9]/[10] for [b]Product[/b] [11] and [b]Location[/b] [12][br]
      also [13] [b]Show Remittables[/b] and [14] [b]Exclude Moved Estimates[/b] and [15] [b]Include Risk Adjustments[/b] and [16] [b]Include Interim Billing[/b][br]
      also [17] [b]Compare Strategy[/b] and [b]Value Inventory[/b] at [18] using [b]Inventory[/b] [19] [20] [b]Fallback to Market[/b][br]
      [b]Deal Number[/b] [21] and [b]Deal Detail Type[/b] [22][br]
      [23] Comparing to Snapshot [24][br]
      [25] Include What-If Scenarios [26]

    </CriteriaUIDefinition>
    <CriteriaUIClass></CriteriaUIClass>
  </CriteriaObject>
  <Control DisplayStyle="SnapshotIncludingInvalidReportComboBox" ID="1" ParameterName="@i_P_EODSnpShtID" Width="250" ShowEmptyText="false" />
  <Control DisplayStyle="PortfolioOnlyComboBox" ID="2" ParameterName="@vc_P_PrtflioID" Width="250" ShowEmptyText="false" />
  <Control DisplayStyle="InternalBusinessAssociateTreeComboBox" ID="3" ParameterName="@vc_PrntBAID" Width="200" />
  
  <Control DisplayStyle="MTVPnLAccountingPeriodFromComboBox" ID="4" ParameterName="@sdt_AccntngPrdID_From" Width="120"/>
  <Control DisplayStyle="MTVPnLAccountingPeriodToComboBox" ID="5" ParameterName="@sdt_AccntngPrdID_To" Width="120"/>
  <Control DisplayStyle="MTVPnLVETradePeriodFromComboBox" ID="6" ParameterName="@sdt_DeliveryPeriod_From" Width="120"/>
  <Control DisplayStyle="MTVPnLVETradePeriodToComboBox" ID="7" ParameterName="@sdt_DeliveryPeriod_To" Width="120"/>
  <Control DisplayStyle="MTVPnLReverseRebooksComboBox" ID="8" ParameterName="@c_ReverseRebooks" Width="140"/>

  <Control DisplayStyle="TransactionGroupComboBox" ID="9" ParameterName="@i_XGrpID" Width="150" />
  <Control DisplayStyle="TransactionTypesWithGroupCSVComboBox" ID="10" ParameterName="@vc_TrnsctnTypID" Width="150" />
  <Control DisplayStyle="ProductComboBox" ID="11" ParameterName="@vc_PrdctID" Width="150" />
  <Control DisplayStyle="LocationComboBox" ID="12" ParameterName="@vc_LcleID" Width="150" />

  <Control DisplayStyle="CheckBox" ID="13" ParameterName="@c_ShowRemittableTaxes" Width="15"/>
  <Control DisplayStyle="CheckBox" ID="14" ParameterName="@c_ExcludeMovedEstimates" Width="15"/>
  <Control DisplayStyle="CheckBox" ID="15" ParameterName="@c_RiskAdjustments" Width="15"/>
  <Control DisplayStyle="CheckBox" ID="16" ParameterName="@c_InterimBilling" Width="15"/>

  <Control DisplayStyle="CheckBox" ID="17" ParameterName="@c_IncludeStrategyInCompare" Width="15"/>
  <Control DisplayStyle="MTVPnLInventoryValueComboBox" ID="18" ParameterName="@c_InventoryValue" Width="140"/>
  <Control DisplayStyle="MTVPnLInventoryOptionsComboBox" ID="19" ParameterName="@c_InventoryOptions" />
  <Control DisplayStyle="CheckBox" ID="20" ParameterName="@c_FallBackToMkt" Width="15"/>
  
  <Control DisplayStyle="TextBox" ID="21" ParameterName="@vc_DealNumber" />
  <Control DisplayStyle="DealDetailTemplateComboBox" ID="22" ParameterName="@vc_DlDtlTmplteID" />

  <Control DisplayStyle="CheckBox" ID="23" ParameterName="@CompareToSnapshot" Width="15"/>
  <Control DisplayStyle="SnapshotIncludingInvalidReportComboBox" ID="24" ParameterName="@i_P_EODSnpShtID_Compare" />
  
  <Control DisplayStyle="CheckBox" ID="25" ParameterName="@IncludeWhatIf" Width="15"/>
  <Control DisplayStyle="WhatIfDealScenarioComboBox" ID="26" ParameterName="@i_WhatIfScenario" />

</ReportConfiguration>