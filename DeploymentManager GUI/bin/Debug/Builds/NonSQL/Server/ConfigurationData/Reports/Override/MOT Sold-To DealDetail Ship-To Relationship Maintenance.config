<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT Sold-To DealDetail Ship-To Relationship Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchSoldToDealDetail, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>DealDetail</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVSAPSoldToDealDetailReportEditManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Trading, Physical Trading</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show SAP SoldTo - ShipTo for [b]Our Business Associate[/b] [1] [br]
      and [b]Their Business Associate[/b] [2] [br]
      and [b]Sold-To Number[/b] [3] [br]
      at [b]Locale[/b] [4] for [b]Product[/b] [5] [br]
      between [b]Deal Dates[/b] [6] and [7] [br]
      between [b]Deal Detail Dates[/b] [8] and [9] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="1" ParameterName="@IntBAID" />
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="2" ParameterName="@ExtBAID" />
  <Control DisplayStyle="MTVSAPBASoldToVendorNumberNoBARelationStaticDescriptionComboBox" ID="3" ParameterName="@SoldTo" />
  <Control DisplayStyle="LocationComboBox" ID="4" ParameterName="@LocaleID" />
  <Control DisplayStyle="ProductComboBox" ID="5" ParameterName="@ProductID" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@DealFromDate" />
  <Control DisplayStyle="DateTime" ID="7" ParameterName="@DealToDate" />
  <Control DisplayStyle="DateTime" ID="8" ParameterName="@DealDetailFromDate" />
  <Control DisplayStyle="DateTime" ID="9" ParameterName="@DealDetailToDate" />
</ReportConfiguration>