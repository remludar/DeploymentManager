<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<ReportDefinition>
		<ReportName>MOT SAP AP Staging Maintenance</ReportName>
		<Class>Motiva.RightAngle.Server.Accounting.Reports.SearchSAPAPStaging, Motiva.RightAngle.Server.Accounting.Reports</Class>
		<BusinessDomain>Accounting</BusinessDomain>
    <ReportEntity>MTVSAPAPStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
		<ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Accounting.EditableReports.MTVSAPAPStagingReportEditManager, Motiva.RightAngle.Client.Accounting.EditableReports</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<ShowReportInNavigation>true</ShowReportInNavigation>
		<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Payable Invoices for [b]External BA[/b] [0] [br]
      for [b]Invoice number[/b] [1] with [b]invoice date[/b] between [2] and [3] [br]
      and a [b]RightAngle interface status[/b] of [4] [br]
      and a [b]Middleware interface status[/b] of [5] [br]
      and an [b]SAP interface status[/b] of [6] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateExternalAllDistinctComboBox" ID="0" ParameterName="@ExternalBAID" />
  <Control DisplayStyle="TextBox" ID="1" ParameterName="@InvoiceNumber" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@InvoiceFromDate" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@InvoiceToDate" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="4" ParameterName="@RAInterfaceStatus" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="5" ParameterName="@MWInterfaceStatus" />
  <Control DisplayStyle="MTVSAPInterfaceStatusComboBox" ID="6" ParameterName="@SAPInterfaceStatus" />
</ReportConfiguration>