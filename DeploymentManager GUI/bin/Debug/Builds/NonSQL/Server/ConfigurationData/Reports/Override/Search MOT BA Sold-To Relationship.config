<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT BA Sold-To Relationship</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchBASoldTo, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Reference Data</BusinessDomain>
    <ReportEntity>MTVSAPBASoldTo</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Trading, Physical Trading</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show SAP BA - SoldTo for [b]Business Associate[/b] [1] [br]
      and [b]Sold To[/b] [2]  [br]
      and [b]Class Of Trade[/b] [3] [br]
      and [b]Credit Lock[/b] [4] [br]
      with [b]Status[/b] [5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="1" ParameterName="@BAID" />
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@SoldTo" />
  <Control DisplayStyle="MTVSAPBASoldToClassOfTradeComboBox" ID="3" ParameterName="@ClassOfTrade" />
  <Control DisplayStyle="YesNoCharComboBox" ID="4" ParameterName="@CreditLock" />
  <Control DisplayStyle="ActiveInactiveComboBox" ID="5" ParameterName="@Status" />
</ReportConfiguration>