<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT STL-FSM Transfer Prices - YT02 - YT10</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchSTLFSMTariffPrices, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing</BusinessDomain>
    <ReportEntity>#MTVSTLFSMTariffPrice</ReportEntity>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Reporting.Criteria.DateReportCriteriaManager, SolArc.RightAngle.Client</ReportCriteriaManagerClass>
    <IncludeInToolsList>>Pricing &amp; Valuation</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Transfer Prices for Transfers from [b] Internal Business Associate [/b] [0] To [b] Internal Business Associate [/b] [1] [br]
      for [b]Cost Categories: [/b][2] at [b]Locations: [/b][3] [br]
      for [b]Products: [/b][4] as of [b]Date [/b][5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="BusinessAssociateIntradivisionalComboBox" ID="0" ParameterName="@i_FromBAId" Width="200" />
  <Control DisplayStyle="BusinessAssociateIntradivisionalComboBox" ID="1" ParameterName="@i_ToBAId" Width="200" />

  <Control DisplayStyle="TransferPriceTypeComboBox" ID="2" ParameterName="@vc_TransferPriceTypes" Filter="DynLstBxTyp NOT IN ('YT01')"/>
  <Control DisplayStyle="LocationComboBox" ID="3" ParameterName="@vc_LocaleIds" Width="200"/>
  <Control DisplayStyle="ProductComboBox" ID="4" ParameterName="@vc_ProductIds" Width="200" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@sdt_DateTime" Width="200" />
</ReportConfiguration>