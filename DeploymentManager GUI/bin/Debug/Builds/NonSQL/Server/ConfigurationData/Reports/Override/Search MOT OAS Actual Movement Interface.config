<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT OAS Actual Movement Interface</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchOASActualMovement, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVOASActualStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show OAS Actual with a [b]Ticket Status [/b] [0] and a [b]Movement Date between[/b] [1] - [2] [br]
      and [b]BOL Number[/b] [3] and [b] RA Location [/b] [4] and [b] RA Product [/b] [5] [br]
      and [b] Import Date  between [/b] [6] - [7]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="0" ParameterName="@TicketStatus" Width="100"/>
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@MovementDateStart" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@MovementDateEnd" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@BOLNumber" Width="200"/>
  <Control DisplayStyle="LocationComboBox" ID="4" ParameterName="@Location" Width="150"/>
  <Control DisplayStyle="ProductComboBox" ID="5" ParameterName="@ProductCode" Width="150"/>
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="7" ParameterName="@InterfaceEnd" />
</ReportConfiguration>