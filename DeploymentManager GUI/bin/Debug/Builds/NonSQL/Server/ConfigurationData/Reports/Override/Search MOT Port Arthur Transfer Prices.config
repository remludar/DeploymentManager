<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Port Arthur Transfer Prices</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchPrtArthurTransferPrices, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing</BusinessDomain>
    <ReportEntity>#MTVPtArthurTrnsfrPrice</ReportEntity>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ReportCriteriaManagerClass>Motiva.RightAngle.Client.General.CriteriaManagers.PortArthurTransferPriceReportCriteriaManager, 
                                Motiva.RightAngle.Client.General.CriteriaManagers</ReportCriteriaManagerClass>
    <IncludeInToolsList>>Pricing &amp; Valuation</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      
      Show Transfer Prices for Transfers from [b] Internal Business Associate [/b][0] To [b] Internal Business Associate [/b][1]  [br]
      of [b] Product [/b] [2] at [b] Location [/b][3] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  
  <Control DisplayStyle="MotivaDivisionsComboBox" ID="0" ParameterName="@i_FromBAId" Width="200"/>
  <Control DisplayStyle="MotivaDivisionsComboBox" ID="1" ParameterName="@i_ToBAId" Width="200" />

  <Control DisplayStyle="MTVProductsComboBox" ID="2" ParameterName="@i_ProductIds" Width="200" />
  <Control DisplayStyle="MTVProductLocationComboBox" ID="3" ParameterName="@i_LocaleIds" Width="200" />
</ReportConfiguration>

