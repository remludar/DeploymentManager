<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>MOT Transfer Price Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchTransferPriceStagingReport, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <ReportEntity>MTVTransferPriceStaging</ReportEntity>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Reporting.Criteria.DateReportCriteriaManager, SolArc.RightAngle.Client</ReportCriteriaManagerClass>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Pricing &amp; Risk</IncludeInToolsList>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Pricing.EditableReports.MTVTransferPriceStagingReportEditManager, Motiva.RightAngle.Client.Pricing.EditableReports</ReportEditManagerClass>    
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Prices for [b] Price Curve is[/b] [0] and [b] Curve Type [/b] [1] where [b]Interface Date [/b] [2] between [3] [br]
      [b] Quote From Date [/b] [4] between [5] and [b] Quote To Date [/b] [6] between [7] [br]
      and [b] Plant [/b] Equals [8] and [b] SAP Material Code [/b] Equals [9] [br]
      with [b] Interface Status [/b] Equals [10]
      
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVPriceCurveNameComboBox" ID="0" ParameterName="@PriceCurveID" />
  <Control DisplayStyle="FPSConditionCodeComboBox" ID="1" ParameterName="@CurveType" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@InterfaceEnd" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@QuoteFromStart" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@QuoteFromEnd" />
  <Control DisplayStyle="DateTime" ID="6" ParameterName="@QuoteToStart" />
  <Control DisplayStyle="DateTime" ID="7" ParameterName="@QuoteToEnd" />
  <Control DisplayStyle="MTVSAPPlantCodeComboBox" ID="8" ParameterName="@PlantCode" />
  <Control DisplayStyle="MTVSAPMaterialCodeComboBox" ID="9" ParameterName="@SAPMaterialCode" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="10" ParameterName="@InterfaceStatus" Width="100" />
 </ReportConfiguration>
