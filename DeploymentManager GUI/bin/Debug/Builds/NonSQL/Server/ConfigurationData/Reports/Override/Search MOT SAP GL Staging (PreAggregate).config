<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<ReportDefinition>
		<ReportName>Search MOT SAP GL Staging (PreAggregate)</ReportName>
		<Class>Motiva.RightAngle.Server.Accounting.Reports.SearchSAPGLStagingPreAggregate, Motiva.RightAngle.Server.Accounting.Reports</Class>
		<BusinessDomain>Accounting</BusinessDomain>
    <ReportEntity>MTVSAPGLStagingPreAggregate</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
		<AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<ShowReportInNavigation>true</ShowReportInNavigation>
		<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show GL Entries for [b]Account Detail ID[/b] [0] with [b]Accounting Period[/b] [1] and [b]Posting Date[/b] [8] [br]
      for [b]GL Account[/b] [6] and [b]Reference Document[/b] of [7] [br]
      and [b]Plant Code[/b] of [2] and [b]Material Code[/b] of [3] [br]
      and [b]Cost Center[/b] of [4] and [b]Profit Center[/b] of [5] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>

  <Control DisplayStyle="TextBox" ID="0" ParameterName="@AcctDtlID" />
  <Control DisplayStyle="AccountingPeriodComboBox" ID="1" ParameterName="@AccntngPrdID" />
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@PlantCode" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@MaterialCode" />
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@CostCenter" />
  <Control DisplayStyle="TextBox" ID="5" ParameterName="@ProfitCenter" />
  <Control DisplayStyle="TextBox" ID="6" ParameterName="@Account" />
  <Control DisplayStyle="TextBox" ID="7" ParameterName="@RefDoc" />
  <Control DisplayStyle="DateTimeShowTime" ID="8" ParameterName="@PostDate" />
</ReportConfiguration>