<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT TMS Orders Staging</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchTMSNominationStaging, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVTMSAccountToTmsStaging</ReportEntity>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show items [b]Created[/b] between [0] and [1] [br]
      with a [b]Scheduled Date[/b] between [2] and [3] [br]
      and a [b]Terminal[/b] [4][br]
      and a [b]PO Number[/b] [5][br]
      and a [b]Product[/b] [6][br]
      and a [b]Customer[/b][7][br]
      and a [b]InterfaceStatus of[/b] [8][br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="DateTime" ID="0" ParameterName="@InterfaceFromDate" />
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@InterfaceToDate" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@FromDate" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@ToDate" />
  <Control DisplayStyle="MTVTMSPlantCodeComboBox"  ID="4" ParameterName="@termid" />
  <Control DisplayStyle="TextBox"  ID="5" ParameterName="@pono" />
  <Control DisplayStyle="MTVTMSProductCodeComboBox"  ID="6" ParameterName="@productid" />
  <Control DisplayStyle="MTVTMSSoldToComboBox"  ID="7" ParameterName="@custno" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox"  ID="8" ParameterName="@interfacestatus" />
</ReportConfiguration>

