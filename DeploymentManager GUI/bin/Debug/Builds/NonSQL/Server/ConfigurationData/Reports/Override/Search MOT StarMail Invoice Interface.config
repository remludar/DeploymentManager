<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
	<ReportDefinition>
		<ReportName>Search MOT StarMail Invoice Interface</ReportName>
		<Class>Motiva.RightAngle.Server.Accounting.Reports.SearchStarMailInvoiceInterface, Motiva.RightAngle.Server.Accounting.Reports</Class>
		<BusinessDomain>Accounting</BusinessDomain>
		<DataBaseConnectionName></DataBaseConnectionName>
		<ExternalCriteriaName></ExternalCriteriaName>
		<AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<ShowReportInNavigation>true</ShowReportInNavigation>
		<RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
		<!--<SortOrder></SortOrder>-->
		<!--<IncludeInToolsList>Accounting</IncludeInToolsList>-->
	</ReportDefinition>
	<CriteriaObject>
		<CriteriaName>ReportCriteria</CriteriaName>
		<CriteriaUIDefinition>
      Show Invoices with an [b]Invoice Number[/b] of [0] [br]
      with a [b]Product Code[/b] of [1] and an [b]Invoice Date[/b] between [2] and [3] [br]
      with an [b]Internal BA[/b] of [4] and an [b]External BA[/b] of [5] [br]
    </CriteriaUIDefinition>
		<CriteriaUIClass />
	</CriteriaObject>
  <Control DisplayStyle="TextBox" ID="0" ParameterName="@InvoiceNumber" />
  <Control DisplayStyle="MTVProductCodeComboBox" ID="1" ParameterName="@ProductCode" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@InvoiceFromDate" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@InvoiceToDate" />
  <Control DisplayStyle="BusinessAssociateInternalWithAllComboBox" ID="4" ParameterName="@InternalBAID" />
  <Control DisplayStyle="BusinessAssociateExternalAllDistinctComboBox" ID="5" ParameterName="@ExternalBAID" />
</ReportConfiguration>
