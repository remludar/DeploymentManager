<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT Exchange Trades Staging</ReportName>
    <Class>Custom.OLF.RightAngle.Server.ExchangeIntegration.Reports.CustomExchangeTradedDealReport, Custom.OLF.RightAngle.Server.ExchangeIntegration</Class>
    <BusinessDomain>Other</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ReportEntity>ExchangeTradedDeal</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>SolArc.RightAngle.Client.Integration.ExchangeTradedDealReportEditManager, SolArc.RightAngle.Client.Integration</ReportEditManagerClass>
    <IncludeInToolsList>Derivative Trading</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Search for Trades from [b]Exchange[/b] [0] from [b]Source[/b] [9] for [b]Symbol[/b] [3][br]
      by [b]Trader[/b] [2] with [b]Status[/b] [1] and [b]BuySell[/b] [4] and [b]PutCall[/b] [5][br]
      for [b]Trade Date[/b] between [6] and [7] with [b]Exchange Trade ID[/b] [8]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="ExchangeComboBox" ID="0" ParameterName="@ExchangeId"  Filter="ExchangeType = 'E'"/>
  <Control DisplayStyle="ExchangeTradedDealStatusTypeComboBox" ID="1" ParameterName="@Status" Width="100"/>
  <Control DisplayStyle="InternalContactComboBox" ID="2" ParameterName="@TraderId" />
  <Control DisplayStyle="ProductInstrumentComboBox" ID="3" ParameterName="@ProductInstrumentId" />
  <Control DisplayStyle="BuySellComboBox" ID="4" ParameterName="@BuySell" Width="50"/>
  <Control DisplayStyle="PutCallComboBox" ID="5" ParameterName="@PutCall" Width="50"/>
  <Control DisplayStyle="DateTimeShowTime" ID="6" ParameterName="@TradeFromDate" />
  <Control DisplayStyle="DateTimeShowTime" ID="7" ParameterName="@TradeToDate" />
  <Control DisplayStyle="TextBox" ID="8" ParameterName="@TransactionId" Width="200"/>
  <Control DisplayStyle="ExchangeSourceSystemComboxBox" ID ="9" ParameterName="@SourceSystemId"/>
  
</ReportConfiguration>