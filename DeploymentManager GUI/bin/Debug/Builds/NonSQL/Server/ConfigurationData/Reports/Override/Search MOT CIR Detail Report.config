<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT CIR Detail Report</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchCIRDetailReport, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>MTVCirDetail</ReportEntity>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInLauncher>false</ShowReportInLauncher>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVCIRReportEditDetailManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
    <IncludeInToolsList></IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show [b]Deal Hdr ID[/b] [0] [br]
      with [b]Revision Level[/b] [1] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="Number0" ID="0" ParameterName="@DlHdrID"/>
  <Control DisplayStyle="Number0" ID="1" ParameterName="@RevisionLevel"/>
</ReportConfiguration>