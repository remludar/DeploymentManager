<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Product Seasonality</ReportName>
    <Class>Motiva.RightAngle.Server.ReferenceData.Reports.ProductSeasonalitySearchReport, Motiva.RightAngle.Server.ReferenceData</Class>
    <BusinessDomain>Reference Data</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ReportEntity>MTVProductSeasonality</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <IncludeInToolsList>Reference Data</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Product Locations where [b]Product[/b] [0] and [b]Location[/b] [1] where [b]SAP Material Number[/b] [2][br]
      where [b]SAP Plant Number[/b] [3] where the [b]County[/b] is [4] and [b]State[/b] is [5] [br]
      and [b]EPA From Date [/b] between [6] to [7] and [b]EPA To Date [/b] between [8] to [9][br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="PositionGroupByProductComboBox" ID="0" ParameterName="@i_ProductID" Filter="SubType IN ('', 'P')"/>
  <Control DisplayStyle="PositionGroupByLocationComboBox" ID="1" ParameterName="@i_LocaleID" Width="100"/>
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@i_SAPMaterialNumber"  Width="200"/>
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@i_SAPPlantNumber" Width="200" />
  <!--<Control DisplayStyle="PositionGroupByLocationComboBox" ID="4" ParameterName="@i_County" Width="100"/>-->
  <!--<Control DisplayStyle="PositionGroupByLocationComboBox" ID="5" ParameterName="@i_State" Width="100"/>-->
  <Control DisplayStyle="PositionGroupByLocationComboBox" ID="4" ParameterName="@i_County" Width="100" Filter="FullPath like '%> State%' and LocaleTypeID=103" />
  <Control DisplayStyle="PositionGroupByLocationComboBox" ID="5" ParameterName="@i_State" Width="100" Filter="FullPath Like '%> State%' and Level=1" />
  <Control DisplayStyle="MTVTMonthDayComboBox" ID="6" ParameterName="@i_EPAFromDate_From" Width="75" />
  <Control DisplayStyle="MTVTMonthDayComboBox" ID="7" ParameterName="@i_EPAFromDate_To" Width="75"/>
  <Control DisplayStyle="MTVTMonthDayComboBox" ID="8" ParameterName="@i_EPAToDate_From" Width="75"/>
  <Control DisplayStyle="MTVTMonthDayComboBox" ID="9" ParameterName="@i_EPAToDate_To" Width="75"/>
  
</ReportConfiguration>