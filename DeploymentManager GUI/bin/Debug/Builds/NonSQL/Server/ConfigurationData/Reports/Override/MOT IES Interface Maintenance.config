<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>MOT IES Interface Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchMTVIESMovement, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVIESExchangeStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Scheduling.EditableReportManagers.MTVIESExchangeStagingReportEditManager, Motiva.RightAngle.Client.Scheduling.EditableReportManagers</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Scheduling, Pricing &amp; Risk</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Exchange Difference Transactions with a [b] DealHeaderInternalNumber [/b] [1] and [b] Movement Header Type [/b] [2] and [br]
      [b] Movement Header Date [/b] [3] between [4] and [b] External Company Code [/b] [5] and [br]
      [b] Product [/b] [6] and [b] Origin [/b] [7] and [b] Destination [/b] [8] and [br]
      [b] Accounting Period [/b] [9] [b] Record Status [/b] [10]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="TextBox" ID="1" ParameterName="@DealHeaderInternalNumber" Width="200"/>
  <Control DisplayStyle="TextBox" ID="2" ParameterName="@MovementHeaderType" Width="150"/>
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@InterfaceEnd" />
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="5" ParameterName="@ExternalBAId" />
  <Control DisplayStyle="ProductComboBox" ID="6" ParameterName="@ProductId" />
  <Control DisplayStyle="PhysicalLocationComboBox" ID="7" ParameterName="@OriginLocId" />
  <Control DisplayStyle="PhysicalLocationComboBox" ID="8" ParameterName="@DestinationLocId" />
  <Control DisplayStyle="TextBox" ID="9" ParameterName="@AccountingPeriod" Width="150"/>
  <Control DisplayStyle="TextBox" ID="10" ParameterName="@RecordStatus" Width="150"/>
</ReportConfiguration>