<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT OAS Inventory Movement Interface</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchOASInventoryMovement, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVOASInventoryStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show OAS Inventory with a [b]Ticket Status [/b] [0] [br]
      and [b]SAP Material No[/b] [1] and [b]Location [/b] [2] and [b] Product [/b] [3] [br]
      and a [b] Import Date between [/b] [4] - [5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="0" ParameterName="@TicketStatus" Width="100"/>
  <Control DisplayStyle="TextBox" ID="1" ParameterName="@SAPMaterialNo" Width="200"/>
  <Control DisplayStyle="LocationComboBox" ID="2" ParameterName="@Location" Width="150"/>
  <Control DisplayStyle="ProductComboBox" ID="3" ParameterName="@ProductCode" Width="150"/>
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@InterfaceEnd" />
</ReportConfiguration>