<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT CIR Report</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchCIRReport, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>#MTVContractChange</ReportEntity>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
	<HideReportInReportDesktop>false</HideReportInReportDesktop>
	<IsReferenceDataReport>false</IsReferenceDataReport>
    <IncludeInToolsList>Trading, Physical Trading</IncludeInToolsList>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVCIRReportEditManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Deals with a [b]Deal Template[/b] is [1] and [b]Status[/b] is [2] [br]
      and are [b]Valid[/b] from [3] to [4] and the [b]Deal Number[/b] [5] [br]
      with [b]Their Company [/b] [6] and our [b] Division [/b] [7] [br]
      and was [b]Created[/b] between [8] and [9] by [b]Our Contact[/b] [10] for [b] Product [/b] [11] [br]
      at [b]Location[/b] [12] using [b] Sub Type [/b] [13] and [b]Receipt/Delivery[/b] [14] [br]
      where the [b] Origin Location [/b] is [15] and the [b] Destination Location [/b] is [16] [br]
      and [b]Show records [/b] [0] and is [b] Emptoris Deal [/b] [17]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVCIRVisibleTypesComboBox" ID="0" ParameterName="@ShowValues"/>"
  <Control DisplayStyle="EntityTemplateComboBox" ID="1" ParameterName="@DealHeaderTemplateID" Width="150"  Filter="TemplateName NOT IN ('Lease', 'Lease Maintenance', 'LeaseProductSource', 'Lease Setup', 'Scheduling Workbench', 'Write Off', 'RailcarRider') AND TemplateName NOT LIKE '%Element%' AND TemplateName NOT LIKE '%Movement%'"/>
  <Control DisplayStyle="DealStatusComboBox" ID="2" ParameterName="@Status"  Width="75" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@DealFromDate" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@DealToDate" />
  <Control DisplayStyle="TextBox" ID="5" ParameterName="@ContractNumber" Width="100"/>
  <Control DisplayStyle="BusinessAssociateAllUnfilteredComboBox" ID="6" ParameterName="@ExternalBAID" Width="300" Filter="Type IN ('X', 'C', 'D', 'A')" />
  <Control DisplayStyle="BusinessAssociateIntradivisionalComboBox" ID="7" ParameterName="@InternalBAID" Width="300"/>
  <Control DisplayStyle="DateTime" ID="8" ParameterName="@DealCreatedFromDate"/>
  <Control DisplayStyle="DateTime" ID="9" ParameterName="@DealCreatedToDate"/>
  <Control DisplayStyle="InternalContactComboBox" ID="10" ParameterName="@InternalContactID"  />
  <Control DisplayStyle="ProductComboBox" ID="11" ParameterName="@Product" Width="200" />
  <Control DisplayStyle="LocationComboBox" ID="12" ParameterName="@Location" Width="200"/>
  <Control DisplayStyle="SubTypeComboBox" ID="13" ParameterName="@SubType" Width="200"/>
  <Control DisplayStyle="ReceiptDeliveryComboBox" ID="14" ParameterName="@DetailReceiptDeliveryType" Width="100"/>
  <Control DisplayStyle="LocationComboBox" ID="15" ParameterName="@OriginLocation" Width="200"/>
  <Control DisplayStyle="LocationComboBox" ID="16" ParameterName="@DestinationLocation" Width="200"/>
  <Control DisplayStyle="YesNoBooleanComboBox" ID="17" ParameterName="@IsEmptorisDeal" Width="100"/>
</ReportConfiguration>