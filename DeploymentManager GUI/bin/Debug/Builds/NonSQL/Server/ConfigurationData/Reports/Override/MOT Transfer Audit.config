<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.
     <Class> - The fully qualified name of the class and assembly that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>MOT Transfer Audit</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchMOTTransferAuditReport, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <ReportEntity>#MOTTransferAudit</ReportEntity>
    <BusinessDomain>Scheduling</BusinessDomain>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Scheduling.EditableReportManagers.SearchMOTTransferAuditReportEditManager, Motiva.RightAngle.Client.Scheduling.EditableReportManagers</ReportEditManagerClass>
    <IncludeInToolsList>Scheduling</IncludeInToolsList>
    <IsReferenceDataReport>false</IsReferenceDataReport>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Deals with a [b]Deal Number[/b] [0] and [b]Detail ID[/b] [1] [br]
      that was [b]Changed[/b] between [2] and [3]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="TextBox" ID="0" ParameterName="@DealNumber"/>"
  <Control DisplayStyle="Number0" ID="1" ParameterName="@DealDetailID"/>"
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@FromChangeDate" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@ToChangeDate" />
</ReportConfiguration>