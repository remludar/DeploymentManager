<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT DealDetail Ship-To Relationship Maintenance</ReportName>
    <Class>Motiva.RightAngle.Server.Trading.Reports.SearchDealDetailShipTo, Motiva.RightAngle.Server.Trading.Reports</Class>
    <BusinessDomain>Trading</BusinessDomain>
    <ReportEntity>MTVDealDetailShipTo</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <ReportEditManagerClass>Motiva.RightAngle.Client.Trading.EditableReports.MTVDealDetailShipToReportEditManager, Motiva.RightAngle.Client.Trading.EditableReports</ReportEditManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <!--    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity> -->
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Trading, Physical Trading</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show DealDetail - ShipTo for [b]Deal Internal Number[/b] [0] [br]
      between [b]Deal Header Date[/b] [2] and [3] [br]
      for [b]Business Associate[/b] [4] [br]
      and [b]Sold To[/b] [5] and [b]Ship To[/b] [6] [br]
      at [b]Ship-To Locale[/b] [7] for [b]Product[/b] [1] [br]
      between [b]DealDetail ShipTo Dates[/b] [8] and [9] [br]
      with [b]Status[/b] [10]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="TextBox" ID="0" ParameterName="@DlHdrIntrnlNbr" Width="100"/>
  <Control DisplayStyle="ProductComboBox" ID="1" ParameterName="@ProductID" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@DlHdrFrmDte" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@DlHdrToDte" />
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="4" ParameterName="@DlHdrExtrnlBAID" />
  <Control DisplayStyle="MTVSAPBASoldToVendorNumberNoBARelationStaticDescriptionComboBox" ID="5" ParameterName="@SoldTo" />
  <Control DisplayStyle="TextBox" ID="6" ParameterName="@ShipTo" />
  <Control DisplayStyle="LocationComboBox" ID="7" ParameterName="@RALocaleID" />
  <Control DisplayStyle="DateTime" ID="8" ParameterName="@SoldToShipToFromDate" />
  <Control DisplayStyle="DateTime" ID="9" ParameterName="@SoldToShipToToDate" />
  <Control DisplayStyle="ActiveInactiveComboBox" ID="10" ParameterName="@Status" />
</ReportConfiguration>