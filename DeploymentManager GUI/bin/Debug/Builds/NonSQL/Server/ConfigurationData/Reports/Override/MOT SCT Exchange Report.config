<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>MOT SCT Exchange Report</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SCTExchangeReport, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>PlannedTransfer</ReportEntity>
    <!--<ReportEditManagerClass>SolArc.RightAngle.Client.Scheduling.EditableReports.InventoryRundown.SchedulersInventoryManagementReportEditManager,SolArc.RightAngle.Client.Scheduling</ReportEditManagerClass>-->
    <ReportEditManagerClass>Motiva.RightAngle.Client.Scheduling.EditableReportManagers.SCTExchangeReportEditManager,Motiva.RightAngle.Client.Scheduling.EditableReportManagers</ReportEditManagerClass>
    <ReportCriteriaManagerClass>SolArc.RightAngle.Client.Scheduling.ReportCriteriaManagers.InventoryRundownReportCriteriaManager, SolArc.RightAngle.Client.Scheduling</ReportCriteriaManagerClass>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <IncludeInToolsList>Scheduling</IncludeInToolsList>
    <HasGroupByClause>true</HasGroupByClause>
    <ReportsToExcludeFromNavigation>Search Inventory Transactions</ReportsToExcludeFromNavigation>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Inventories for [b]Product[/b] [0] at [b]Location[/b] [1][br]
      for [b]Position Group[/b] [2] where the [b]Deal Number[/b] [6] [br]
      with the [b]Date Range[/b] [3] for the [b]Dates[/b] from [4] to [5][br]
      for the [b]Deal Types[/b] [7] with [b]Minimum Expired Balance[/b] [8] and [b]Maximum Expired Months[/b] [9] [br]
      where [b]Related[/b] is [10] and where [b]By Day[/b] is [11]
    </CriteriaUIDefinition>
  </CriteriaObject>
  <Control DisplayStyle="PositionGroupByProductComboBox" ID="0" ParameterName="@Product" Width="300" Filter="SubType IN ('', 'P')"/>
  <Control DisplayStyle="PositionGroupByLocationComboBox" ID="1" ParameterName="@Location" Width="300" />
  <Control DisplayStyle="PositionGroupExtendedComboBox" ID="2" ParameterName="@PositionGroup"  Width="200" />
  <Control DisplayStyle="DynamicDateRangeComboBox" ID="3" ParameterName="@DateRange" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@FromDate" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@ToDate" />
  <Control DisplayStyle="TextBox" ID="6" ParameterName="@DealInternalNumber" Width="100"/>
  <Control DisplayStyle="DealTypeComboBox" ID="7" ParameterName="@IncludeDealTypes" Width="200" Filter="Description NOT LIKE ('FX%') AND Description NOT LIKE ('%Future%') AND Description NOT LIKE ('%Option%') AND Description NOT IN ('EFP Deal', 'EFS Deal', 'Lease', 'Master Agreement', 'Swap Deal', 'Swap Inhouse Deal')"/>
  <Control DisplayStyle="Number2" ID="8" ParameterName="@MinExpiredBalance" Width="40"/>
  <Control DisplayStyle="Number0" ID="9" ParameterName="@MaxExpiredMonths" Width="40"/>
  <Control DisplayStyle="YesNoComboBox" ID="10" ParameterName="@ShowRelated" Width="50" ShowEmptyText="false"/>
  <Control DisplayStyle="YesNoComboBox" ID="11" ParameterName="@ShowByDay" Width="50" ShowEmptyText="false"/>
</ReportConfiguration>