<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT T4 Interface</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchT4Nomination, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVT4NominationHeaderStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show [b]Nomination Type[/b] [0] [b]Interfaced[/b] between [1] and [2] [br]
      For [b]Scheduled Date[/b] [3] to [4] for [b] Carrier [/b] [5] or [b] Shipper [/b] [6] or [b] Tankage  [/b] [7][br]
      or [b]Consignee[/b] [8] with Product [9] at Location [10] with Status [11]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVT4NominationTypeComboBox" ID="0" ParameterName="@Nomination"/>
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@InterfaceStart" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@InterfaceEnd" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@ScheduledStart"/>
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@ScheduledEnd"/>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="5" ParameterName="@Carrier" Width="150"/>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="6" ParameterName="@Shipper" Width="150"/>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="7" ParameterName="@Tankage" Width="150"/>
  <Control DisplayStyle="BusinessAssociateAllComboBox" ID="8" ParameterName="@Consignee" Width="150"/>
  <Control DisplayStyle="ProductComboBox" ID="9" ParameterName="@Product" Width="150"/>
  <Control DisplayStyle="LocationComboBox" ID="10" ParameterName="@Location" Width="150"/>
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="11" ParameterName="@RecordStatus" Width="100"/>
</ReportConfiguration>