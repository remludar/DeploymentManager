<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT DataLake Inventory Balances</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchDataLakeInventoryBalances, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Accounting</BusinessDomain>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Data Lake Inventory Balances for [b]Storage Location[/b] [0] [br]
      and a [b]Deal Number[/b] of [1] and [b]Accounting Period[/b] of [2] [br]
      and [b]Product Group[/b] of [3] and [b]Product Name[/b] of [4] [br]
      and [b]Trading Book[/b] of [5] and a [b]Status[/b] of [6]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVLocaleAbbvTextComboBox" ID="0" ParameterName="@StorageLocation" Width="200"/>
  <Control DisplayStyle="TextBox" ID="1" ParameterName="@DealNumber" />
  <Control DisplayStyle="MTVDataLakeAcctPrdComboBox" ID="2" ParameterName="@AccountingPeriod" />
  <Control DisplayStyle="CommodityComboBox" ID="3" ParameterName="@ProductGroup" />
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@ProductName" />
  <Control DisplayStyle="TextBox" ID="5" ParameterName="@TradingBook" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="6" ParameterName="@Status" />
</ReportConfiguration>