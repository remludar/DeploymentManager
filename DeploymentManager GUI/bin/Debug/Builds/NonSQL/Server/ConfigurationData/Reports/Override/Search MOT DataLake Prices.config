<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT DataLake Prices</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchDataLakePrices, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Pricing and Valuation</BusinessDomain>
    <ReportEntity>RawPriceDetail</ReportEntity>
    <IsReferenceDataReport>false</IsReferenceDataReport>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <IncludeInToolsList>Pricing &amp; Risk</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Prices [b]Effective[/b] for Today offset by [0] days [br]
    </CriteriaUIDefinition>
  </CriteriaObject>
  <Control DisplayStyle="TextBox" ID="0" ParameterName="@EffectiveDateOffset" />
</ReportConfiguration>