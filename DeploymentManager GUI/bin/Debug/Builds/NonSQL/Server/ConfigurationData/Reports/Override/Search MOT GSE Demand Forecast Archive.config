<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT GSE Demand Forecast Archive</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.MTVGSEDemandForecastArchive, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVDemandForecastArchive</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <ShowReportInLauncher>true</ShowReportInLauncher>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Rack Demand Forecast Archive with a Creation Date [/b] [1] between [2]
      [b]Forecast Type[/b] [3] and
      [b]Product [/b] [4] and[br]
      [b]Location [/b] [5] and
      [b]Contract Type [/b] [6] and[br]
      [b]Business Associate [/b] [7]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@CreationDateStart" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@CreationDateEnd" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@ForecastType" Width="100"/>
  <Control DisplayStyle="ProductComboBox" ID="4" ParameterName="@Product" Width="100"/>
  <Control DisplayStyle="LocationComboBox" ID="5" ParameterName="@Location" Width="100"/>
  <Control DisplayStyle="TextBox" ID="6" ParameterName="@ContractType" Width="100"/>
  <Control DisplayStyle="TextBox" ID="7" ParameterName="@BusinessAssociate" Width="100"/>
</ReportConfiguration>