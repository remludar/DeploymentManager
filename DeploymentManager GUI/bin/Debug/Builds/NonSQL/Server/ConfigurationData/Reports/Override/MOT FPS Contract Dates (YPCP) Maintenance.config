<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
	<!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
	<ReportDefinition>
		<ReportName>MOT FPS Contract Dates (YPCP) Maintenance</ReportName>
		<Class>Motiva.RightAngle.Server.Trading.Reports.SearchFPSContractDatesYPCP, Motiva.RightAngle.Server.Trading.Reports</Class>
		<BusinessDomain>Pricing and Valuation</BusinessDomain>
		<ReportEntity>MTVFPSContractPriceDates</ReportEntity>
		<DataBaseConnectionName></DataBaseConnectionName>
		<ExternalCriteriaName></ExternalCriteriaName>
		<ReportEditManagerClass>Motiva.RightAngle.Client.Pricing.EditableReports.FPSContractDatesYPCPReportEditManager, Motiva.RightAngle.Client.Pricing.EditableReports</ReportEditManagerClass>
		<AllowOneToManyNavigation>true</AllowOneToManyNavigation>
		<HideReportInReportDesktop>false</HideReportInReportDesktop>
		<ShowReportInNavigation>false</ShowReportInNavigation>
		<ShowReportInLauncher>true</ShowReportInLauncher>
		<RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>
		<!--<SortOrder></SortOrder>-->
		<!--<IncludeInToolsList>Accounting &amp; Risk</IncludeInToolsList>-->
		<IncludeInToolsList>Trading, Pricing &amp; Risk</IncludeInToolsList>
	</ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show prices [b]Interfaced[/b] between [0] and [1] [br]
      for [b]Plant[/b] [4] with [b]Material Code[/b] [5] for [b]SoldTo[/b] [6] [br]
      at [b]ShipTo[/b] [7] and [b]Condition Type[/b] [11] [br]
      with [b]Status[/b] [10]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="DateTime" ID="0" ParameterName="@InterfaceFromDate" />
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@InterfaceToDate" />
  <Control DisplayStyle="TextBox" ID="4" ParameterName="@PlantCode" />
  <Control DisplayStyle="TextBox" ID="5" ParameterName="@MaterialCode" />
  <Control DisplayStyle="TextBox" ID="6" ParameterName="@SoldTo" />
  <Control DisplayStyle="TextBox" ID="7" ParameterName="@ShipTo" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="10" ParameterName="@RecordStatus" />
  <Control DisplayStyle="TextBox" ID="11" ParameterName="@ConditionType" />
</ReportConfiguration>