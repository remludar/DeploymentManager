<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Orion Nominations</ReportName>
    <Class>Motiva.RightAngle.Server.Scheduling.Reports.SearchOrionNominations, Motiva.RightAngle.Server.Scheduling.Reports</Class>
    <BusinessDomain>Scheduling</BusinessDomain>
    <ReportEntity>MTVOrionNominationStaging</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Scheduling, Movement</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Search Orion Nominations rows at [b]Location [/b] [0] [br]
      for [b]Product [/b] [1] [br]
      and [b]Batch Number [/b] [2] and [b]Trip Number [/b] [3] [br]
      between [b]Pump Date Range[/b] from [4] to [5]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="LocationComboBox" ID="0" ParameterName="@LocaleID" Width="100"/>
  <Control DisplayStyle="ProductComboBox" ID="1" ParameterName="@PrdctID" />
  <Control DisplayStyle="MTVOrderBatchNumber" ID="2" ParameterName="@BatchNumber" />
  <Control DisplayStyle="MTVOrderTripNumber" ID="3" ParameterName="@TripNumber" />
  <Control DisplayStyle="DateTime" ID="4" ParameterName="@PUMP_DATE_FROM" />
  <Control DisplayStyle="DateTime" ID="5" ParameterName="@PUMP_DATE_To" />
</ReportConfiguration>