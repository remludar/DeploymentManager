<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <ReportDefinition>
    <ReportName>Search MOT Tax Rule Audit</ReportName>
    <Class>Motiva.RightAngle.Server.Accounting.Reports.MTVTaxAuditReport, Motiva.RightAngle.Server.Accounting.Reports</Class>
    <BusinessDomain>Tax</BusinessDomain>
    <ReportEntity></ReportEntity>
    <ReportCriteriaManagerClass></ReportCriteriaManagerClass>
    <AllowOneToManyNavigation>false</AllowOneToManyNavigation>
    <ShowReportInNavigation>false</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>false</RequiresBusinessAssociateSecurity>   
    <IncludeInToolsList>Accounting, Tax</IncludeInToolsList>
    <ReportsToExcludeFromNavigation></ReportsToExcludeFromNavigation>
  </ReportDefinition>
	<CriteriaObject>
		<CriteriaName>ReportCriteria</CriteriaName>
		<CriteriaUIDefinition>
			Show Tax Audit logs [b]from [/b][0] [b]to[/b] [1] [br]
			with [b]User[/b] [2] and [b]Tax Ruleset[/b] of [3] [br]
			and [b]Tab changed[/b] of [4]
		</CriteriaUIDefinition>
		<CriteriaUIClass></CriteriaUIClass>
	</CriteriaObject>
	<Control DisplayStyle="DateTime" ID="0" ParameterName="@d_FromDate"  Width="125" />
	<Control DisplayStyle="DateTime" ID="1" ParameterName="@d_ToDate"  Width="125"/>
	<Control DisplayStyle="UserComboBox" ID="2" ParameterName="@i_User"/>
  <!--<Control DisplayStyle="MTVTaxAuditRuleSetDescriptionComboBox" ID="3" ParameterName="@i_TaxRuleSet"/>-->
  <Control DisplayStyle="MTVLastKnownTaxRleStDescComboBox" ID="3" ParameterName="@vc_TaxRuleSet" Width="300"/>
  <Control DisplayStyle="MTVTaxAuditTabListComboBox" ID="4" ParameterName="@vc_TabChanged"/>
</ReportConfiguration>


