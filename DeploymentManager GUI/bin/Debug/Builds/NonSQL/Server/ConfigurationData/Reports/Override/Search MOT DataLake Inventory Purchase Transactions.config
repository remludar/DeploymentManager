<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT DataLake Inventory Purchase Transactions</ReportName>
    <Class>Motiva.RightAngle.Server.Pricing.Reports.SearchDataLakeInventoryTransactions, Motiva.RightAngle.Server.Pricing.Reports</Class>
    <BusinessDomain>Accounting</BusinessDomain>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList>Accounting</IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show Data Lake Inventory Purchase Transactions for [b]Material Number[/b] [0] [br]
      for [b]BOL Move Dates[/b] between [1] and [2] [br]
      and a [b]Deal Number[/b] of [3] and [b]Accounting Period[/b] of [4] [br]
      and [b]Product Group[/b] of [5] and a [b]Status[/b] of [6] [br]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="MTVSAPMaterialCodeTextComboBox" ID="0" ParameterName="@MaterialNumber" />
  <Control DisplayStyle="DateTime" ID="1" ParameterName="@BOLMoveFromDate" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@BOLMoveToDate" />
  <Control DisplayStyle="TextBox" ID="3" ParameterName="@DealNumber" />
  <Control DisplayStyle="MTVDataLakeAcctPrdComboBox" ID="4" ParameterName="@AccountingPeriod" />
  <Control DisplayStyle="CommodityComboBox" ID="5" ParameterName="@ProductGroup" />
  <Control DisplayStyle="MTVInterfaceStatusComboBox" ID="6" ParameterName="@Status" />
</ReportConfiguration>