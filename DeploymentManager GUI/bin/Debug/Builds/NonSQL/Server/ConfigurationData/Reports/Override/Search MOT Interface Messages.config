<ReportConfiguration xmlns="http://tempuri.org/ReportConfiguration.xsd">
  <!--
    Set the attributes used to describe a report definition.
     <ReportName> - The name associated with the report.  Appears in the Treeview and on the tab when the report is opened.
     <Class> - The fully qualified name of the class and assembly (public class) that defines the report.
     <FolderName> - The name of the folder where the report resides.
    -->
  <ReportDefinition>
    <ReportName>Search MOT Interface Messages</ReportName>
    <Class>Motiva.RightAngle.Server.Interface.Nexus.Report.SearchInterfaceMessages, Motiva.RightAngle.Server.Interface.Nexus.Report</Class>
    <BusinessDomain>Feeds</BusinessDomain>
    <ReportEntity>#MTVInterfaceMessages</ReportEntity>
    <DataBaseConnectionName></DataBaseConnectionName>
    <ExternalCriteriaName></ExternalCriteriaName>
    <AllowOneToManyNavigation>true</AllowOneToManyNavigation>
    <ShowReportInNavigation>true</ShowReportInNavigation>
    <RequiresBusinessAssociateSecurity>true</RequiresBusinessAssociateSecurity>
    <!--<SortOrder></SortOrder>-->
    <IncludeInToolsList></IncludeInToolsList>
  </ReportDefinition>
  <CriteriaObject>
    <CriteriaName>ReportCriteria</CriteriaName>
    <CriteriaUIDefinition>
      Show interface messages for [b]Interface Router[/b] [1] [br]
      and [b]Internal Message ID[/b] [0] [br]
      [b]Created[/b] between [2] and [3]
    </CriteriaUIDefinition>
    <CriteriaUIClass />
  </CriteriaObject>
  <Control DisplayStyle="Number0" ID="0" ParameterName="@InternalMessageID" Width="80" />
  <Control DisplayStyle="TextBox" ID="1" ParameterName="@Router" Width="200" />
  <Control DisplayStyle="DateTime" ID="2" ParameterName="@CreatedFromDate" />
  <Control DisplayStyle="DateTime" ID="3" ParameterName="@CreatedToDate" />
</ReportConfiguration>
